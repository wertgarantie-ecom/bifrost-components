{
  "name": "wertgarantie-rating",
  "version": "2.0.1",
  "description": "wertgarantie web component to display google review ratings",
  "main": "dist/rating.min.js",
  "scripts": {
    "version": "sed -i \"\" -E \"s/(this.componentVersion = \\')(.*)(\\';)/\\1$(node -p 'require(\"./package.json\").version')\\3/\" ./src/rating.js && git add ./src/rating.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "faucet --compact",
    "fingerprint": "faucet --fingerprint",
    "watch": "faucet -w",
    "serve": "faucet --liveservice 9999"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wertgarantie-ecom/bifrost-components.git"
  },
  "keywords": [
    "e-commerce",
    "webcomponent",
    "wertgarantie",
    "rating",
    "google"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wertgarantie-ecom/bifrost-components/issues"
  },
  "homepage": "https://github.com/wertgarantie-ecom/bifrost-components#readme"
}
