  "version": "2.0.52",
  "description": "wertgarantie web component to select policy products",
  "main": "selection-popup.js",
  "scripts": {
    "version": "sed -i \"\" -E \"s/(this.componentVersion = \\')(.*)(\\';)/\\1$(node -p 'require(\"./package.json\").version')\\3/\" ./src/selection-popup.js && git add ./src/selection-popup.js",
